   "source": [
    "### Create a custom dtype that describes a color as four unisgned bytes (RGBA) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "color = np.dtype([\n",
    "    (\"r\", np.ubyte, 1), \n",
    "    (\"g\", np.ubyte, 1), \n",
    "    (\"b\", np.ubyte, 1), \n",
    "    (\"a\", np.ubyte, 1)])"
   ]
  },
